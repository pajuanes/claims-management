{
  "name": "bdeo-claims-management",
  "version": "1.0.0",
  "type": "module",
  "description": "Node.js backend for BDEO claims management",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "dev:backend": "npm --prefix backend/node-backend run dev",
    "dev:frontend": "npm --prefix frontend run start",
    "install:backend": "npm --prefix backend/node-backend install",
    "install:frontend": "npm --prefix frontend install",
    "build:frontend": "npm --prefix frontend run build",
    "build:frontend:gh-pages": "npm --prefix frontend run build -- --base-href=/claims-management/",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.13.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
